CREATE DATABASE STORE;
USE STORE;
CREATE TABLE STOREEMPL(empno INT PRIMARY KEY, ename varchar(20), job varchar(20), mgr int(10), hiredate DATE, sal DECIMAL(10, 2),comm DECIMAL(10, 2),deptno INT);
SELECT * FROM STOREEMPL;
