SELECT ename, sal FROM STOREEMPL WHERE sal >= 2200;
SELECT * FROM STOREEMPL WHERE comm IS NULL;
SELECT ename, sal FROM STOREEMPL WHERE sal NOT BETWEEN 2500 AND 4000;
SELECT ename, job, sal FROM STOREEMPL WHERE mgr IS NULL;
SELECT ename FROM STOREEMPL WHERE ename LIKE '__A%';
SELECT ename FROM STOREEMPL WHERE ename LIKE '%T';
